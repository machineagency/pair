{
  "compilerOptions": {
    "target": "es2021",
    "module": "es2020",
    "sourceMap": true,
    "moduleResolution": "node",
    "strict": true,
    "noImplicitAny": true,
    "jsx": "react",
    "allowSyntheticDefaultImports": true,
    "allowUmdGlobalAccess": true
  },
  "include": [
      "client/*.ts", "client/*.tsx", "./*.ts"
  ],
  "typeRoots": [
      "node_modules/@types"
  ]
}
